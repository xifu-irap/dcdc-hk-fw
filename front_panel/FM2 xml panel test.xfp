<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Test EGSE AF FM2 -->
<resource version="2.3.0.1">
<object class="okPanel" name="panel1">
<title>EGSE AF FM 2 ON/OFF controls</title>
<size>400,220</size>

<!-- ON/OFF Shutter -->
<object class="okToggleButton">
<label>Shutter ON/OFF</label>
<position>10,10</position>
<size>100,-1</size>
<endpoint>0x04</endpoint>
<bit>0</bit>
</object>

<!-- Statut shutter -->
<object class="okLED">
<position>135,10</position>
<size>100,30</size>
<label align="top">Shutter status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>11</bit>
</object>

<!-- ON/OFF Motor -->
<object class="okToggleButton">
<label>Motor ON/OFF</label>
<position>10,50</position>
<size>100,-1</size>
<endpoint>0x13</endpoint>
<bit>0</bit>
</object>

<!-- Statut Motor -->
<object class="okLED">
<position>135,50</position>
<size>100,30</size>
<label align="top">Motor status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>15</bit>
</object>

<!-- ON/OFF Micro -->
<object class="okToggleButton">
<label>Micro ON/OFF</label>
<position>10,90</position>
<size>100,-1</size>
<endpoint>0x18</endpoint>
<bit>0</bit>
</object>

<!-- Statut Micro -->
<object class="okLED">
<position>135,90</position>
<size>100,30</size>
<label align="top">Micro status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>14</bit>
</object>

<!-- ON/OFF Heater -->
<object class="okToggleButton">
<label>CWL heater ON/OFF</label>
<position>10,130</position>
<size>100,-1</size>
<endpoint>0x19</endpoint>
<bit>0</bit>
</object>

<!-- Statut Heater -->
<object class="okLED">
<position>135,130</position>
<size>100,30</size>
<label align="top">CWL heater status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>13</bit>
</object>

<!-- ON/OFF CWL -->
<object class="okCombobox">
<position>10,170</position>
<size>100,-1</size>
<options>
<item value="0">OFF</item>
<item value="2">CWL ON</item>
<item value="5">CWL + Modulation</item>
</options>
<endpoint>0x05</endpoint>
<bit>0</bit>
</object>

<!-- Statut CWL -->
<object class="okLED">
<position>135,170</position>
<size>100,30</size>
<label align="top">CWL status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>10</bit>
</object>

<object class="okLED">
<position>235,170</position>
<size>100,30</size>
<label align="top">Modulation status</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x20</endpoint>
<bit>9</bit>
</object>

<!-- version-->
<object class="okDigitDisplay">
<position>250,10</position>
<size>100,-1</size>
<maxvalue>65535</maxvalue>
<radix>16</radix>
<endpoint>0x23</endpoint>
<bit>0</bit>
</object>

</object>

<!-- Housekeepings -->
<object class="okPanel" name="panel2">
<title>Housekeepings</title>
<size>400,200</size>

<!-- ID de la HK Ã  prendre -->
<object class="okCombobox">
<position>10,10</position>
<size>150,-1</size>
<options>
<item value="0">AF signal</item>
<item value="1">Mic signal</item>
<item value="2">CWL Power</item>
<item value="3">Mic temperature</item>
<item value="4">LPD signal</item>
<item value="5">CWL temperature</item>
<item value="6">MP temperature</item>
<item value="7">Motor temperature</item>
</options>
<endpoint>0x15</endpoint>
<bit>0</bit>
</object>

<!-- prise HK -->
<object class="okTriggerButton">
<label>Get HK</label>
<position>170,10</position>
<size>60,20</size>
<endpoint>0x40</endpoint>
<bit>1</bit>
</object>

<!-- prise AF -->
<object class="okTriggerButton">
<label>Get AF</label>
<position>240,10</position>
<size>60,20</size>
<endpoint>0x40</endpoint>
<bit>4</bit>
</object>


<object class="okFilePipe">
<label>record AF file</label>
<position>300,10</position>
<size>60,20</size>
<endpoint>0xa0</endpoint>
<length>726</length>
<starttrigger><endpoint>0x40</endpoint><bit>4</bit></starttrigger>
</object>



<object class="okTriggerLog">
<position>10,150</position>
<size>350,500</size>
<trigger>
<endpoint>0x6A</endpoint><bit>0</bit>
<message>HK envoyee!</message>
</trigger>
<trigger>
<endpoint>0x6A</endpoint><bit>3</bit>
<message>AF envoye!</message>
</trigger>
</object>

<!-- Retour HK ID-->
<object class="okDigitDisplay">
<position>10,50</position>
<size>50,30</size>
<maxvalue>7</maxvalue>
<radix>10</radix>
<endpoint>0x20</endpoint>
<bit>0</bit>
</object>

<!-- Retour HK valeur -->
<object class="okDigitDisplay">
<position>100,50</position>
<size>300,30</size>
<maxvalue>64535</maxvalue>
<radix>10</radix>
<endpoint>0x21</endpoint>
<bit>0</bit>
</object>

</object>



<!-- Moteur -->
<object class="okPanel" name="panel3">
<title>Motor</title>
<size>450,350</size>

<!-- Bypass -->
<object class="okToggleButton">
<label>Bypass ON/OFF</label>
<position>10,10</position>
<size>-1,-1</size>
<endpoint>0x10</endpoint>
<bit>12</bit>
</object>


<!-- Boost -->
<object class="okToggleButton">
<label>Boost ON/OFF</label>
<position>10,50</position>
<size>-1,-1</size>
<endpoint>0x10</endpoint>
<bit>2</bit>
</object>

<!-- Direction -->
<object class="okCombobox">
<position>10,90</position>
<size>-1,-1</size>
<options>
<item value="0">Close Focus</item>
<item value="1">Infinite Focus</item>
</options>
<endpoint>0x11</endpoint>
<bit>15</bit>
</object>

<object class="okDigitEntry">
<position>10,130</position>
<size>200,30</size>
<tooltip>Number of steps</tooltip>
<minvalue>0</minvalue>
<maxvalue>65535</maxvalue>
<value>1000</value>
<endpoint>0x12</endpoint>
<bit>0</bit>
</object>

<!-- motor move -->
<object class="okTriggerButton">
<label>Motor Move</label>
<position>220,130</position>
<size>-1,-1</size>
<endpoint>0x40</endpoint>
<bit>0</bit>
</object>


<!-- Statut IF -->
<object class="okLED">
<position>120,10</position>
<size>100,30</size>
<label align="top">IF switch</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x30</endpoint>
<bit>1</bit>
</object>

<!-- Statut CF -->
<object class="okLED">
<position>240,10</position>
<size>100,30</size>
<label align="top">CF switch</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x30</endpoint>
<bit>0</bit>
</object>

<!-- Vitesse -->
<object class="okCombobox">
<position>200,90</position>
<size>-1,-1</size>
<options>
<item value="0">10 pas/s</item>
<item value="1">50 pas/s</item>
<item value="2">100 pas/s</item>
<item value="3">150 pas/s</item>
</options>
<endpoint>0x10</endpoint>
<bit>0</bit>
</object>

<!-- Retour MSB steps done valeur -->
<object class="okDigitDisplay">
<position>10,200</position>
<size>300,30</size>
<maxvalue>64535</maxvalue>
<radix>10</radix>
<endpoint>0x31</endpoint>
<bit>0</bit>
</object>

<!-- Retour LSB steps done valeur -->
<object class="okDigitDisplay">
<position>10,240</position>
<size>300,30</size>
<maxvalue>64535</maxvalue>
<radix>10</radix>
<endpoint>0x32</endpoint>
<bit>0</bit>
</object>


<!-- deplacement done -->
<object class="okLED">
<position>350,200</position>
<size>100,30</size>
<label align="top">Moving</label>
<style>ROUND</style>
<color>#00ff00</color>
<endpoint>0x30</endpoint>
<bit>15</bit>
</object>

</object>

</resource>